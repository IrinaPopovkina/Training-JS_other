https://www.codewars.com/kata/5635e7cb49adc7b54500001

function solve(n) {
  let count = 0;
  if(n % 10 !== 0){
    return -1;
  }
  while(n >= 500){n = n - 500;count++;}
  while(n >= 200){n = n - 200;count++;}
  while(n >= 100){n = n - 100;count++;}
  while(n >= 50){n = n - 50;count++;}
  while(n >= 20){n = n - 20;count++;}
  while(n >= 10){n = n - 10;count++;}
    return count;
  }
